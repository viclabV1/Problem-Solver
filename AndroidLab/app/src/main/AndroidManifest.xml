<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.problemsolver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".PuzzleProblemActivity"
            android:parentActivityName=".EightPuzzleActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EightPuzzleActivity" />
        </activity>

        <activity android:name=".FifteenProblemActivity"
            android:parentActivityName=".FifteenActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FifteenActivity"/>
        </activity>
        <activity
            android:name=".FWGCProblemActivity"
            android:parentActivityName=".FWGCActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FWGCActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FWGCActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".EightPuzzleActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".FifteenActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>